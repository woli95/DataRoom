     <button type=\"button\" v-on:click=\"changeView('enter')\" class=\"btn blue col s4\" style=\"margin: 10px 0 0 0;\">ENTER BUILDING</button>\n      <div class=\"col s4\"/>\n      <button type=\"button\" v-on:click=\"changeView('create')\" class=\"btn blue col s4\" style=\"margin: 10px 0 0 0;\">CREATE NEW BUILDING</button>\n    </div>\n    <hr style=\"color: deepskyblue\"/>\n    <enter-building-div v-if=\"buildingEnterView\" class=\"row\">\n      <div v-if=\"building_list !== null\" class=\"col s4\">\n        <h3 class=\"center-align title\">Choose building</h3>\n        <ul>\n          <li v-for=\"(item, index) in building_list\" :key=\"index\" class=\"list_item\"> {{ item }}</li>\n        </ul>\n      </div>\n      <div v-if=\"floor_list !== null\" class=\"col s4\">\n        <h3 class=\"center-align title\">Choose building</h3>\n        <ul>\n          <li v-for=\"(item, index) in building_list\" :key=\"index\" class=\"list_item\"> {{ item }}</li>\n        </ul>\n      </div>\n    </enter-building-div>\n    <create-building-div v-if=\"buildingCreateView\"/>\n    <admin-building-div v-if=\"buildingAdminView\"/>\n  </div>\n</template>\n\n<script>\nimport createBuildingDiv from '@/components/createBuildingDiv';\nimport enterBuildingDiv from '@/components/enterBuildingDiv';\nimport adminBuildingDiv from '@/components/adminBuildingDiv';\nexport default {\n  name: \"dashboard\",\n  components: {\n    createBuildingDiv, enterBuildingDiv, adminBuildingDiv\n  },\n  data() {\n    return {\n      building_list: null,\n      floor_list: null,\n      buildingEnterView: false,\n      buildingCreateView: false,\n      buildingAdminView: false,\n    }\n  },\n  methods: {\n    changeView(view) {\n      if (view === 'enter') {\n        this.buildingEnterView = true;\n        this.buildingCreateView = false;\n        this.buildingAdminView = false;\n      } else if (view === 'create') {\n        this.buildingCreateView = true;\n        this.buildingEnterView = false;\n        this.buildingAdminView = false;\n      } else if (view === 'admin') {\n        this.buildingCreateView = false;\n        this.buildingEnterView = false;\n        this.buildingAdminView = true;\n      }\n    },\n    async getBuildingList() {\n      const result = await this.$root.getBuildingList();\n      this.building_list = result[1];\n    }\n  },\n  async beforeMount() {\n    await this.getBuildingList();\n  }\n}\n</script>\n\n<style scoped>\n#main {\n  border-right: 5px outset #468499;\n  box-shadow: 4px 4px 4px 4px #468199\n}\n.title {\n  font-size: 25px;\n  font-family: Roboto,serif;\n  color: blue;\n  text-shadow: 1px 2px aqua;\n}\n.list_item {\n  padding: 3px;\n  margin: 7px;\n  background-color: red;\n  border-radius: 10px;\n}\n#active_list_item {\n\n}\n</style>"],"sourceRoot":""}]}