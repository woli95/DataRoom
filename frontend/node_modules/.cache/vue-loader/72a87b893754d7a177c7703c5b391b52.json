n      profileSettingsView: false,\n      dashboardView: false,\n      tokenInfoView: false,\n      currentClientInfo: null,\n    }\n  },\n  async mounted() {\n    await axios.get(this.$root.$data.backend_url.concat('/client/', this.$root.$data.fb.auth().currentUser.uid.toString(), '/info')).then((response) => {\n      console.log(response);\n    });\n  },\n  computed: {\n    console: () => console,\n  },\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}