ton type=\"button\" v-on:click=\"enterBuilding('user')\" class=\"btn btn-large blue darken-2 col s4 left\">As User</button>\n      <button type=\"button\" v-on:click=\"enterBuilding('admin')\" class=\"btn btn-large blue darken-2 col s4 right\">As Admin</button>\n      <div class=\"row\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport M from 'materialize-css';\nexport default {\n  name: \"enterBuildingDiv\",\n  data() {\n    return {\n      list: {\n        building_list: null,\n        floor_list: null,\n        room_list: null,\n      },\n      currently: {\n        building: null,\n        floor: null,\n        room: null,\n      },\n      tmp: {\n        val1: null,\n      }\n    }\n  },\n  methods: {\n    selectBuilding(building_item) {\n      this.tmp.val1 = building_item;\n      if (building_item[\"isAdmin\"] === false)\n        this.enterBuilding('user', building_item);\n      else if (building_item[\"isAdmin\"] === true)\n        M.Modal.init(document.getElementById('buildingEnterAdminOrUser')).open();\n    },\n    async enterBuilding(asWho) {\n      M.Modal.getInstance(document.getElementById('buildingEnterAdminOrUser')).close();\n      if (asWho === 'user') {\n        this.currently.building = this.tmp.val1[\"buildingName\"];\n        await this.getFloorList(this.tmp.val1[\"buildingName\"])\n        this.$forceUpdate();\n      }\n      else if (asWho === 'admin') {\n        this.$parent.changeView('admin', this.tmp.val1[\"buildingName\"]);\n      }\n\n    },\n    async getBuildingList() {\n      const result = await this.$root.getBuildingList();\n      this.list.building_list = result[1];\n    },\n    async getFloorList(name) {\n      const result = await this.$root.getBuildingFloors(name);\n      this.list.floor_list = result[1];\n      console.log(this.list.floor_list);\n    }\n  },\n  async beforeMount() {\n    await this.getBuildingList();\n  }\n}\n</script>\n\n<style scoped>\n.buildingList {\n  margin: 5px;\n}\n.title {\n  color: white;\n  background-color: blue;\n  padding: 5px;\n}\n.buildingItem {\n  font-size: 30px;\n  text-decoration-style: unset;\n}\n</style>"]}]}